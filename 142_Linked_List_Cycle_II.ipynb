   "source": [
    "def detectCycle(head):\n",
    "    temp = head\n",
    "    tempList = {}\n",
    "    while temp:\n",
    "        tempList[temp] = True\n",
    "        if temp.next in tempList:\n",
    "            return temp.next\n",
    "        temp = temp.next\n",
    "    return None"
   ]
  }
